  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug (Launch) - Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-ContractTestBench<ContractTestBench>",
      "request": "launch",
      "mainClass": "main.ContractTestBench",
      "projectName": "ContractTestBench"
    },
    {
      "type": "java",
      "name": "CodeLens (Launch) - Main",
      "request": "launch",
      "vmArgs": "--module-path M:/Java/JavaFX/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
      "mainClass": "main.ContractTestBench"
  }
  ]
